<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <Button
        android:text="Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="86dp"
        android:layout_marginStart="86dp"
        android:layout_marginBottom="43dp"
        android:id="@+id/VisitarPagina" />

    <Button
        android:text="Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/HacerPedido"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="21dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="50dp"
        android:id="@+id/ENSALADAS" />

    <Spinner
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ENSALADAS"
        android:layout_alignRight="@+id/VisitarPagina"
        android:layout_alignEnd="@+id/VisitarPagina"
        android:layout_marginTop="46dp"
        android:id="@+id/spinner6" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/vegetariana"
        android:layout_below="@+id/spinner6"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="29dp"
        android:id="@+id/imageView11" />

    <Button
        android:text="Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView11"
        android:layout_alignRight="@+id/ENSALADAS"
        android:layout_alignEnd="@+id/ENSALADAS"
        android:layout_marginTop="30dp"
        android:id="@+id/ALMUERZOS" />

    <Spinner
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_alignRight="@+id/spinner6"
        android:layout_alignEnd="@+id/spinner6"
        android:id="@+id/spinner11" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/vegetariana"
        android:layout_below="@+id/spinner11"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"
        android:id="@+id/imageView12" />

    <Button
        android:text="Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView12"
        android:layout_alignRight="@+id/ALMUERZOS"
        android:layout_alignEnd="@+id/ALMUERZOS"
        android:layout_marginTop="39dp"
        android:id="@+id/POSTRES" />

    <Spinner
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/POSTRES"
        android:layout_alignRight="@+id/VisitarPagina"
        android:layout_alignEnd="@+id/VisitarPagina"
        android:layout_marginTop="15dp"
        android:id="@+id/spinner12" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/bocadito"
        android:layout_below="@+id/spinner12"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="14dp"
        android:id="@+id/imageView13" />

    <Button
        android:text="Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/VisitarPagina"
        android:layout_toRightOf="@+id/imageView12"
        android:layout_toEndOf="@+id/imageView12"
        android:layout_marginLeft="66dp"
        android:layout_marginStart="66dp"
        android:id="@+id/PEDIDO" />

</RelativeLayout>